<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="VitruviusApiUrl" value="https://localhost:8000/api/v1" />
    <add key="VitruviusWebUrl" value="https://localhost:3000" />
    <!-- 
    WARNING: DO NOT STORE API KEYS IN PLAIN TEXT
    The add-in will prompt for user credentials instead
    -->
    <add key="AutoOpenWebInterface" value="true" />
    <add key="ExportIFCVersion" value="IFC2x3CV2" />
    <add key="ExportBaseQuantities" value="true" />
    <add key="ExportInternalProperties" value="true" />
    <add key="DeleteTempFiles" value="true" />
    <add key="TokenExpirationMinutes" value="60" />
    <add key="UseWindowsCredentialManager" value="true" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
</configuration>